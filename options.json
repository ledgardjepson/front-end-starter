{
  "parser": "postcss-scss",
  "use": [
    "postcss-strip-inline-comments",
    "postcss-partial-import",
    "postcss-inline-svg",
    "postcss-nested-props",
    "cssnano",
    "precss",
    "postcss-cssnext",
    "postcss-pxtorem"
  ],
  "postcss-partial-import": {
    "prefix": "_",
    "extension": "scss"
  },
  "postcss-inline-svg": {
    "path": "app/images"
  },
  "local-plugins": true
}
